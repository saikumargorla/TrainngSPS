 <h3>Assignment Operators</h3>
 <p id="p1"></p>
 <p id="p2"></p>
 <form>
  <script>
  var num1=1,num2=989 // assignment operator
  var str1="First String" ;var str2="Second String"
  var AddOfNums=num1+num2;
  
  num1+=num2;
  document.getElementById("p1").innerHTML=num1
  str1+=str2;//str2 is added to str1 and result is assigned to str1
  document.getElementById("p2").innerHTML=str1 + "  (str1+=str2)"
  </script>
  </form>
